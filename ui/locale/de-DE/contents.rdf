<?xml version="1.0"?>
<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

  <!-- list all the packages being supplied by this jar -->
  <RDF:Seq about="urn:mozilla:locale:root">
    <RDF:li resource="urn:mozilla:locale:de-DE"/>
  </RDF:Seq>

  <RDF:Seq about="urn:mozilla:locale:root">
    <RDF:li resource="urn:mozilla:locale:de-AT"/>
  </RDF:Seq>

  <!-- locale information -->
  <RDF:Description about="urn:mozilla:locale:de-DE"
        chrome:displayName="Deutsch (Deutschland)"
        chrome:author="mozilla.org"
        chrome:name="de-DE"
        chrome:previewURL="http://www.mozilla.org/locales/de-DE.gif">
    <chrome:packages>
      <RDF:Seq about="urn:mozilla:locale:de-DE:packages">
        <RDF:li resource="urn:mozilla:locale:de-DE:enigmail"/>
      </RDF:Seq>
    </chrome:packages>
  </RDF:Description>

  <RDF:Description about="urn:mozilla:locale:de-AT"
        chrome:displayName="Deutsch"
        chrome:author="mozilla.org"
        chrome:name="de-AT"
        chrome:previewURL="http://www.mozilla.org/locales/de-AT.gif">
    <chrome:packages>
      <RDF:Seq about="urn:mozilla:locale:de-AT:packages">
        <RDF:li resource="urn:mozilla:locale:de-AT:enigmail"/>
      </RDF:Seq>
    </chrome:packages>
  </RDF:Description>

</RDF:RDF>
